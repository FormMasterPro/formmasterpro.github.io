{
  "version": 2,
  "routes": [
    {
      "src": "/assets/styles.css",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/assets/.*\\.js$",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": ".*/videos/.*\\.webm$",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": ".*/images/.*\\.(png|jpg|jpeg|gif|svg|webp)$",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "src": "/(.*)\\.html",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" },
      "continue": true
    }
  ]
}
